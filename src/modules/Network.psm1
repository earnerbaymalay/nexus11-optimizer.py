function Invoke-NetworkOptimizations { param($Config); Write-Host ''; Write-Host '  :: NETWORK OPTIMIZATION' -ForegroundColor $Config.Theme.Title; Draw-Line '-' -Config $Config; Log-Action 'Flushing DNS' 'OK' -Config $Config; Clear-DnsClientCache; Log-Action 'Optimizing TCP Global' 'OK' -Config $Config; netsh int tcp set global autotuninglevel=normal | Out-Null; netsh int tcp set global rss=enabled | Out-Null; Log-Action 'Cleaning Temp Files' 'OK' -Config $Config; Remove-Item -Path "$env:LOCALAPPDATA\Temp\*" -Recurse -Force -ErrorAction SilentlyContinue }
Export-ModuleMember -Function Invoke-NetworkOptimizations
